<div class="container">
  <div class="row">
    <span><%= link_to "Search", new_search_path %></span>
  </div>
</div>

<% if @search.search_items.blank? %>
  <h4>No Records Found</h4>
  <% else %>
  <div class="row">
    <%- @search.search_items.each do |item| %>
      <div class="col-xs-12 col-sm-6">
        <div class="index-img">
          <%= link_to item, class:"thumbnail" do %>
            <%= image_tag item.thumbnail_photo.image_url(:medium) %>
          <% end %>
        </div>
      </div>
    <% end %>
  </div>
<% end %>
